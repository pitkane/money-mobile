{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "restParams": true,
      "spread": true,
      "experimentalObjectRestSpread": true
    },
    "ecmaVersion": 7,
    "sourceType": "module"
  },
  // "ecmaFeatures": {
  // 
  // 
  // 
  // },
  "extends": "eslint:recommended",
  "plugins": [
    "babel",
    "react",
    "prettier"
  ],
  "rules": {
    "no-console": "off",
    "react/jsx-uses-vars": [
      2
    ]
  }
}