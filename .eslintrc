{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "ecmaFeatures": {
    "jsx": true,
    "restParams": true,
    "spread": true
  },
  "extends": "eslint:recommended",
  "plugins": [
    "babel",
    "react",
    "prettier"
  ],
  "rules": {
    "no-console": "off"
  }
}